{
  "name": "react-boilerplate",
  "author": "Tom√°s Catena",
  "license": "MIT",
  "version": "0.0.1",
  "description": "React/Typescript Boilerplate",
  "main": "src/index.tsx",
  "scripts": {
    "start": "webpack serve --config config/webpack.dev.ts",
    "build": "webpack --config config/webpack.prod.ts",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "lint:fix": "eslint src/**/*.ts src/**/*.tsx --fix",
    "lint:quiet": "eslint src/**/*.ts src/**/*.tsx --quiet",
    "lint-staged": "lint-staged --config lint-staged.js",
    "typescript": "tsc --project tsconfig.json --noEmit",
    "husky-install": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/material": "^5.9.3",
    "@types/react-router-dom": "^5.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@types/node": "^18.6.4",
    "@types/react": "^18.0.16",
    "@types/react-dom": "^18.0.6",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "eslint": "^8.21.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "sass-loader": "^13.0.2",
    "ts-node": "^10.9.1",
    "tsc-files": "^1.1.3",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-merge": "^5.8.0"
  },
  "keywords": [
    "react",
    "react-boilerplate",
    "webpack",
    "typescript",
    "react-typescript",
    "material ui",
    "mui",
    "react router dom",
    "eslint"
