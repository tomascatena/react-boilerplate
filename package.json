  "bin": "bin/createReactTsApp.js",
  "description": "React/Typescript Starter Project",
  "main": "src/index.tsx",
  "scripts": {
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "start": "webpack serve --config config/webpack.dev.ts",
    "dev": "webpack serve --config config/webpack.dev.ts",
    "build": "webpack --config config/webpack.prod.ts",
    "build:analyze": "webpack --config config/webpack.prod.ts --env WEBPACK_ANALYZE",
    "lint": "eslint \"**/*.js\" \"**/*.ts\" \"**/*.tsx\" --ignore-pattern node_modules/",
    "lint:fix": "eslint \"**/*.js\" \"**/*.ts\" \"**/*.tsx\" --ignore-pattern node_modules/ --fix",
    "lint:quiet": "eslint \"**/*.js\" \"**/*.ts\" \"**/*.tsx\" --ignore-pattern node_modules/ --quiet",
    "lint-staged": "lint-staged --config lint-staged.js",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "husky-install": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "postinstall": "npx husky install"
  },
